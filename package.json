{
  "name": "stackoverflowclonelite",
  "version": "1.0.0",
  "description": "stack overflow lite clone ",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch server.js --exec babel-node -- server.js",
    "test": "nyc --reporter=html --reporter=text --require babel-polyfill --require babel-register mocha \" api/v1/tests/**/*.spec.js\" --reporter progress --exit ",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": " esw app.js server.js api/v1/* --color",
    "lint-watch": "npm run lint -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anuoluwa/LyteStackOverflow.git"
  },
  "keywords": [
    "node",
    "express",
    "eslint",
    "babel",
    "travisci",
    "code",
    "climate",
    "code",
    "cov"
  ],
  "author": "Anuoluwapo Apiti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Anuoluwa/LyteStackOverflow/issues"
  },
  "homepage": "https://github.com/Anuoluwa/LyteStackOverflow#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-api-versioning": "^2.0.0",
    "express-validator": "^5.3.0",
    "jsdoc": "^3.5.5",
    "jsonwebtoken": "^8.3.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.3",
    "nyc": "^12.0.2",
    "should": "^13.2.3",
    "supertest": "^3.1.0"
  },
  "nyc": {
    "check coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  }
}
